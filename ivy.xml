<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
		xmlns:m="http://ant.apache.org/ivy/maven">
	<info organisation="org.bitbucket.tbrugz" module="sqldump"/>
	<configurations>
		<conf name="default"/>
		<conf name="test" extends="default" visibility="private"/>
		<conf name="findbugs" visibility="private"/>
	</configurations>
	<publications>
		<artifact name="sqldump" type="jar" conf="default"/>
		<artifact name="sqldump" type="source" ext="jar" conf="default" m:classifier="sources"/>
	</publications>
	<dependencies>
		<!-- base deps -->
		<dependency org="commons-logging" name="commons-logging" rev="1.2" conf="default->default"/>
		
		<!-- grabbers/dumpers deps -->
		<dependency name="kmlutils" rev="0.5.5" conf="default->default"/>
		<dependency org="org.codehaus.jettison" name="jettison" rev="1.3.3" conf="default->default"/>
		
		<!-- datadump syntax deps -->
		<dependency org="org.odftoolkit" name="simple-odf" rev="0.6.6"/>
		<dependency org="org.jopendocument" name="jOpenDocument" rev="1.2"/>
		<dependency org="org.apache.poi" name="poi-ooxml" rev="3.14"/>
		
		<!-- mondrian deps -->
		<dependency org="org.olap4j" name="olap4j" rev="1.1.0"/> <!-- XXX conf="default->default" ? -->
		<dependency org="pentaho" name="mondrian" rev="3.6.0" conf="test->default"/> <!-- 3.6.0, 3.12.0.2-208 -->
		<dependency org="eigenbase" name="eigenbase-xom" rev="1.3.1" conf="test->default"/>

		<!-- optional deps -->
		<dependency org="log4j" name="log4j" rev="1.2.16"/>
		<dependency org="org.eclipse.persistence" name="org.eclipse.persistence.moxy" rev="2.4.0"/>

		<!-- optional sqldiff deps -->
		<dependency org="com.googlecode.java-diff-utils" name="diffutils" rev="1.2.1"/>

		<!-- test deps -->
		<dependency org="junit" name="junit" rev="4.12" conf="test->default"/>
		<dependency org="org.apache.derby" name="derby" rev="10.9.1.0" conf="test->default"/>
		<dependency org="org.hsqldb" name="hsqldb" rev="2.2.8" conf="test->default"/>
		<dependency org="com.h2database" name="h2" rev="1.3.174" conf="test->default"/>
		<dependency org="com.googlecode.json-simple" name="json-simple" rev="1.1.1" conf="test->default"/>
		
		<!-- code analysis deps -->
		<dependency org="net.sourceforge.pmd" name="pmd" rev="5.0.4" conf="test->default">
			<exclude org="commons-io" name="commons-io"/>
		</dependency>
		<dependency org="commons-io" name="commons-io" rev="2.5"/>
		<!-- <dependency org="net.sourceforge.pmd" name="pmd-core" rev="5.4.2" conf="test->default"/> -->
		<dependency org="net.sourceforge.cobertura" name="cobertura" rev="2.0.3" conf="test->default"/>
		<dependency org="com.google.code.findbugs" name="findbugs" rev="2.0.1" conf="findbugs->default"/>
		<dependency org="com.puppycrawl.tools" name="checkstyle" rev="5.6" conf="test->default"/>
		<dependency org="jdepend" name="jdepend" rev="2.9.1" conf="test->default"/>
	</dependencies>
</ivy-module>
