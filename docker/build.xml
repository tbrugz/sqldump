<project name="sqldump-docker"
    default="resolve"
    xmlns:ivy="antlib:org.apache.ivy.ant">

    <target name="clean">
        <delete dir="lib"/>
    </target>

    <target name="resolve" description="retrieve dependencies with Ivy">
        <ivy:retrieve/>
    </target>

    <target name="dependencytree" description="show Ivy dependency tree">
        <ivy:dependencytree/>
    </target>

</project>
